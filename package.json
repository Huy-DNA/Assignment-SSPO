{
  "name": "sspo",
  "version": "1.0.0",
  "description": "An SE assignment",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "build:backend": "copyfiles backend/**/* .dist && copyfiles ./index.ejs .dist",
    "build:frontend": "webpack",
    "build": "npx yarn build:backend && npx yarn build:frontend",
    "start:cas": "node ./__mock-cas__/server.js",
    "start": "concurrently \"npx yarn start:cas\" \"node ./.dist/backend/bin/www.js\"",
    "start:cas": "node ./__mock-cas__/server.js",
    "start": "concurrently \"npx yarn start:cas\" \"node ./.dist/backend/bin/www.js\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "watch:backend": "nodemon --watch ./backend --exec \"copyfiles backend/**/* .dist && copyfiles ./index.ejs .dist\"",
    "watch:frontend": "webpack --watch",
    "watch:start": "nodemon --watch .dist --exec \"npx yarn start\"",
    "dev": "concurrently \"npx yarn watch:backend\" \"npx yarn watch:frontend\" \"npx yarn watch:start\"",
    "migrate": "cd backend && npx prisma migrate dev",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HuyDNA/Assignment-SSPO.git"
